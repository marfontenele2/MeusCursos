CASE
  WHEN EQUIPE = 'Caetano Dias Oliveira' THEN 'CDO'
  WHEN EQUIPE = 'ESF do Privat' THEN 'PRIVAT'
  WHEN EQUIPE = 'Sambaiba' THEN 'SAMBAÍBA'
  WHEN EQUIPE = 'Centro de Saude' THEN 'CRO 1'
  WHEN EQUIPE = 'ESF Cro 02' THEN 'CRO 2'
  WHEN EQUIPE = 'Ibuguacu' THEN 'UBATUBA'
  WHEN EQUIPE = 'Csu-02' THEN 'SÃO PEDRO 2'
  WHEN EQUIPE = 'Sede' THEN 'SÃO PEDRO 1'
  WHEN EQUIPE = 'ESF de Paula Pessoa' THEN 'PAULA PESSOA'
  WHEN EQUIPE = 'Pessoa Anta' THEN 'IBUAÇU'
  WHEN EQUIPE = 'Santa Terezinha' THEN 'SANTA TEREZINHA 1'
  WHEN EQUIPE = 'Santa Terezinha 02' THEN 'SANTA TEREZINHA 2'
  WHEN EQUIPE = 'Timonha' THEN 'TIMONHA 1'
  WHEN EQUIPE = 'Timonha 02' THEN 'TIMONHA 2'
  WHEN EQUIPE = 'ESF do Estreito dos Martins' THEN 'ESTREITO'
  WHEN EQUIPE = 'Adrianopoles' THEN 'ADRIANÓPOLES'
  WHEN EQUIPE = 'Parazinho' THEN 'PARAZINHO 1'
  WHEN EQUIPE = 'Parazinho 02' THEN 'PARAZINHO 2'
  WHEN EQUIPE = 'ESF da Portelinha' THEN 'PORTELINHA'
  WHEN EQUIPE = 'ESF do Sao Francisco' THEN 'HPM'
  WHEN EQUIPE = 'ESF da Tiaia' THEN 'TIAIA'
  WHEN EQUIPE = 'CDO' THEN 'CDO'
  WHEN EQUIPE = 'PRIVAT' THEN 'PRIVAT'
  WHEN EQUIPE = 'SAMBAÍBA' THEN 'SAMBAÍBA'
  WHEN EQUIPE = 'CRO 1' THEN 'CRO 1'
  WHEN EQUIPE = 'CRO 2' THEN 'CRO 2'
  WHEN EQUIPE = 'UBATUBA' THEN 'UBATUBA'
  WHEN EQUIPE = 'SÃO PEDRO 1' THEN 'SÃO PEDRO 1'
  WHEN EQUIPE = 'SÃO PEDRO 2' THEN 'SÃO PEDRO 2'
  WHEN EQUIPE = 'PAULA PESSOA' THEN 'PAULA PESSOA'
  WHEN EQUIPE = 'IBUAÇU' THEN 'IBUAÇU'
  WHEN EQUIPE = 'SANTA TEREZINHA 1' THEN 'SANTA TEREZINHA 1'
  WHEN EQUIPE = 'SANTA TEREZINHA 2' THEN 'SANTA TEREZINHA 2'
  WHEN EQUIPE = 'TIMONHA 1' THEN 'TIMONHA 1'
  WHEN EQUIPE = 'TIMONHA 2' THEN 'TIMONHA 2'
  WHEN EQUIPE = 'ESTREITO' THEN 'ESTREITO'
  WHEN EQUIPE = 'ADRIANÓPOLES' THEN 'ADRIANÓPOLES'
  WHEN EQUIPE = 'PARAZINHO 1' THEN 'PARAZINHO 1'
  WHEN EQUIPE = 'PARAZINHO 2' THEN 'PARAZINHO 2'
  WHEN EQUIPE = 'PORTELINHA' THEN 'PORTELINHA'
  WHEN EQUIPE = 'HPM' THEN 'HPM'
  WHEN EQUIPE = 'TIAIA' THEN 'TIAIA'

END